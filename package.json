{
  "name": "@sadbox/walledator",
  "version": "0.2.1",
  "description": "Asynchronous data validation",
  "main": "index.js",
  "author": "Kirill Mesnyankin <sadbox.games@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strayiker/walledator.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint src",
